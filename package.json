{
  "name": "project-demo",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services/**/*.service.js",
    "start": "moleculer-runner",
    "cli": "moleculer connect NATS",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "dc:up": "docker compose up --build -d",
    "dc:logs": "docker compose logs -f",
    "dc:down": "docker compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "prettier": "^3.5.3",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "supertest": "^7.1.1",
    "socket.io-client": "^4.8.1",
    "graphql-request": "^6.1.0",
    "moleculer-repl": "^0.7.4"
  },
  "dependencies": {
    "moleculer-web": "^0.11.0-beta2",
    "@moleculer/database": "^0.3.0",
    "mongodb": "^6.17.0",
    "@seald-io/nedb": "^4.1.1",
    "nats": "^2.29.3",
    "ioredis": "^5.6.1",
    "graphql": "^15.10.0",
    "moleculer-apollo-server": "^0.3.8",
    "graphql-type-json": "^0.3.2",
    "@moleculer/channels": "^0.2.0",
    "@moleculer/workflows": "^0.1.1",
    "moleculer-io": "^2.2.0",
    "lodash": "^4.17.21",
    "moleculer": "^0.15.0-beta1"
  },
  "overrides": {
    "@graphql-tools/links@7": {
      "apollo-upload-client": "15.0.0"
    },
    "@graphql-tools/url-loader@6": {
      "extract-files": "10.0.0"
    },
    "graphql-request": {
      "extract-files": "10.0.0"
    },
    "apollo-upload-client": {
      "extract-files": "10.0.0"
    }
  },
  "engines": {
    "node": ">= 20.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
